  "vulnerabilities": [
    {
      "cve": "CVE-2021-42021",
      "cwe": {
        "id": "CWE-26",
        "name": "Path Traversal: '/dir/../filename'"
      },
      "notes": [
        {
          "category": "summary",
          "text": "The affected application contains a path traversal vulnerability that could allow the reading of arbitrary files on the server located outside the application\u2019s web document directory. An unauthenticated remote attacker could exploit this vulnerability to access sensitive information for subsequent attacks.CVE-2021-42021 has been assigned to this vulnerability. A CVSS v3 base score of 8.6 has been calculated; the CVSS vector string is (AV:N/AC:L/PR:N/UI:N/S:C/C:H/I:N/A:N).",
          "title": "Vulnerability Summary"
        }
      ],
